{
  "name": "hjtechblog",
  "version": "1.0.0",
  "private": true,
  "description": "HJ Tech Blog",
  "author": "hyeongjun <hyeongjun@daily-funding.com>",
  "repository": "hjtechblog",
  "packageManager": "yarn@1.22.22",
  "engines": {
    "node": ">=22"
  },
  "type": "module",
  "scripts": {
    "prebuild": "panda codegen",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "deploy": "vercel deploy",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "test": "run-p test:*",
    "test:lint": "eslint .",
    "test:prettier": "prettier '**/*.{js,jsx,ts,tsx}' --check"
  },
  "dependencies": {
    "@fisch0920/use-dark-mode": "^2.4.0",
    "@keyvhq/core": "^1.6.9",
    "@keyvhq/redis": "^1.6.10",
    "@notionhq/client": "^3.1.3",
    "@react-icons/all-files": "^4.1.0",
    "@supabase/supabase-js": "^2.50.2",
    "@types/crypto-js": "^4.2.2",
    "classnames": "^2.5.1",
    "cookie": "^0.6.0",
    "crypto-js": "^4.2.0",
    "expiry-map": "^2.0.0",
    "fathom-client": "^3.4.1",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "ky": "^1.8.1",
    "lqip-modern": "^2.2.1",
    "next": "^15.3.3",
    "notion-client": "^7.4.2",
    "notion-types": "^7.4.2",
    "notion-utils": "^7.4.2",
    "p-map": "^7.0.3",
    "p-memoize": "^7.1.1",
    "posthog-js": "^1.249.4",
    "prismjs": "^1.30.0",
    "react": "^19.1.0",
    "react-body-classname": "^1.3.1",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-notion-x": "^7.4.2",
    "react-tweet": "^3.2.2",
    "react-use": "^17.6.0",
    "rss": "^1.2.2",
    "supabase": "^2.30.4",
    "swiper": "^11.2.8",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@fisch0920/config": "^1.1.2",
    "@next/bundle-analyzer": "^15.3.3",
    "@pandacss/dev": "^1.4.3",
    "@types/cookie": "^0.6.0",
    "@types/js-cookie": "^3.0.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.15.30",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^19.1.6",
    "@types/react-body-classname": "^1.1.10",
    "@types/rss": "^0.0.32",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.2",
    "eslint": "^9.28.0",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  },
  "overrides": {
    "cacheable-request": {
      "keyv": "yarn:@keyvhq/core@~1.6.6"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --ignore-unknown --write",
      "eslint --fix"
    ]
  }
}
