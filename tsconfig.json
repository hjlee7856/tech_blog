{
  "extends": "@fisch0920/config/tsconfig-react",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "components/*"
      ],
      "@/lib/*": [
        "lib/*"
      ],
      "@/server/*": [
        "server/*"
      ],
      "@/styles/*": [
        "styles/*"
      ],
      "@/store/*": [
        "store/*"
      ],
      "@/hooks/*": [
        "hooks/*"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "*.config.ts",
    "next-env.d.ts",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
